<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net7.0-windows</TargetFramework>
        <UseWPF>true</UseWPF>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <WarningLevel>5</WarningLevel>
        <Version>0.2.0</Version>
        <Authors>Pavel Dubsky</Authors>
        <Company>Padutronics</Company>
        <Product>Padutronics Framework</Product>
        <Description>WPF utilities integration to dependency injection container.</Description>
        <Copyright>Copyright Â© Padutronics 2023</Copyright>
        <PackageProjectUrl>https://github.com/Padutronics/Padutronics.Wpf.DependencyInjection</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Padutronics/Padutronics.Wpf.DependencyInjection</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Padutronics.DependencyInjection" Version="0.1.0" />
        <PackageReference Include="Padutronics.Wpf" Version="0.3.0" />
    </ItemGroup>
</Project>